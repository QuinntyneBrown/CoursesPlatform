{
  "name": "courses-platform.web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "playwright test --config=projects/courses-platform/e2e/playwright.config.ts",
    "e2e:ui": "playwright test --config=projects/courses-platform/e2e/playwright.config.ts --ui",
    "e2e:headed": "playwright test --config=projects/courses-platform/e2e/playwright.config.ts --headed",
    "e2e:debug": "playwright test --config=projects/courses-platform/e2e/playwright.config.ts --debug",
    "e2e:report": "playwright show-report projects/courses-platform/e2e/playwright-report",
    "storybook": "ng run courses-platform-components:storybook",
    "build-storybook": "ng run courses-platform-components:build-storybook"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.4",
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.5",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.4",
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/angular": "^10.1.10",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "storybook": "^10.1.10",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8",
    "zone.js": "^0.16.0"
  }
}
